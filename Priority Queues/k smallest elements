#include<queue>
vector<int> kSmallest(int arr[], int n, int k) {
    // Write your code here
    vector<int> ans;
   priority_queue<int> maxh;
    for(int i=0;i<n;i++){
        maxh.push(arr[i]);
        if(maxh.size()>k){
            maxh.pop();
        }
    }
    while(maxh.size()>0){
        ans.push_back(maxh.top());
        maxh.pop();
    }

    return ans;
}
